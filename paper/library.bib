  journal = {Science},
  year = {2005},
  volume = {307},
  pages = {563--566},
  abstract = {It has been suggested that enhanced anion concentrations at the liquid/vapor
	interface of airborne saline droplets are important to aerosol reactions
	in the atmosphere. We report ionic concentrations in the surface
