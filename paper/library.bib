@article{Coxon2015a,
title = "Improved direct potential fit analyses for the ground electronic states of the hydrogen halides: HF/DF/TF, HCl/DCl/TCl, HBr/DBr/TBr and HI/DI/TI ",
journal = BTjqsrt,
volume = "151",
number = "0",
pages = "133--154",
