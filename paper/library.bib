@Article{Beatriz2008a,
author ="Cordero, Beatriz and Gomez, Veronica and Platero-Prats, Ana E. and Reves, Marc and Echeverria, Jorge and Cremades, Eduard and Barragan, Flavia and Alvarez, Santiago",
