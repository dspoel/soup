@article{Lagardere2015a,
author = {Lagard{\`e}re, Louis and Lipparini, Filippo and Polack, {\'E}tienne and Stamm, Benjamin and Canc{\`e}s, {\'E}ric and Schnieders, Michael and Ren, Pengyu and Maday, Yvon and Piquemal, Jean-Philip},
