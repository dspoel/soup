@article{Mei2015a,
author = {Mei, Ye and Simmonett, Andrew C. and Pickard, Frank C. and DiStasio, Robert A. and Brooks, Bernard R. and Shao, Yihan},
title = {Numerical Study on the Partitioning of the Molecular Polarizability into Fluctuating Charge and Induced Atomic Dipole Contributions},
journal = BTjpca,
volume = {119},
