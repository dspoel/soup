@article{Guissani1994a,
   author = "Guissani, Yves and Guillot, Bertrand",
   title = "Coexisting phases and criticality in NaCl by computer simulation",
   journal = BTjcp,
   year = "1994",
   volume = "101",
   number = "1", 
