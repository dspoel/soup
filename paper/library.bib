@Article{Wiedemair2017a,
author ="Wiedemair, Martin J. and Hofer, Thomas",
title  ="Towards a dissociative SPC-like water model - probing the impact of intramolecular Coulombic contributions",
journal  ="Phys. Chem. Chem. Phys.",
