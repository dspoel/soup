@article{Fumi1964a,
title = "Ionic sizes and born repulsive parameters in the NaCl-type alkali halides-I",
journal = BTjpcs,
volume = "25",
number = "1",
pages = "31 - 43",
year = "1964",
note = "",
issn = "0022-3697",
