@article{DasMahanta2016a,
title = "The effect of monovalent cations on the collective dynamics of water and on a model protein ",
journal = "Journal of Molecular Liquids ",
volume = "215",
number = "",
pages = "197 - 203",
year = "2016",
note = "",
issn = "0167-7322",
doi = "http://dx.doi.org/10.1016/j.molliq.2015.12.031",
url = "http://www.sciencedirect.com/science/article/pii/S0167732215306036",
author = "Debasish Das Mahanta and Nirnay Samanta and Rajib Kumar Mitra",
keywords = "Monovalent cation",
keywords = "Dielectric relaxation",
keywords = "THz time-domain spectroscopy",
keywords = "Collective hydration dynamics",
keywords = "Structure breaker",
keywords = "Protein hydration ",
abstract = "Abstract In this contribution we present experimental evidences of the ultrafast (sub-ps to ps) collective hydrogen bond dynamics of water in the extended hydration layers of alkali metal chlorides by using \{THz\} time domain spectroscopy (TTDS) in the terahertz frequency region (0.3-2.1 THz; 10-70 cm- 1). The behaviour of the frequency dependent absorption coefficient (alpha), refractive index (n) and the dielectric relaxation response time (tau), obtained by fitting both real and imaginary parts of the permittivity (epsilon) using a triple Debye relaxation model are studied. The timescales obtained for bulk water are of the order of~ 9 ps, 200 fs and 80 fs. The relaxation time constant in the presence of the salt shows an accelerated reorientation dynamics of the water dipoles, which, in turn renders an indirect support for the 'water structure breaking' ability of monovalent ions, the effect being ion specific. We have also studied the effect of these ions on the secondary structure as well as hydration dynamics around a model protein bovine serum albumin (BSA). It is found that the structure and hydration of the protein as well as the salt hydration suffer negligible perturbation in the presence of the alkali metal chlorides. "
}

